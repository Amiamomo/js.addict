{
  "name": "ReactWithTest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "examples": "{examples}/*.js"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --inline -w --hot",
    "test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --recursive",
    "tdd": "npm test -- --watch",
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot",
    "build": "webpack -p",
    "ex1": "node examples/extend1.js",
    "example": "node --",
    "examples": "node examples/extend1.js",
    "watch1": "nodemon -e js -x 'node examples/extend1.js'",
    "watch:example": "onchange **/*.js -- npm run example {{changed}}",
    "build:scss": "node-sass --output-style compressed -o dist/css src/scss",
    "watch:scss": "onchange src/scss/*.scss -- npm run build:scss"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "onchange": "^3.2.1",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^2.1.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "css-loader": "^0.28.0",
    "jquery": "^3.2.1",
    "jsdom": "^9.12.0",
    "npm-watch": "^0.1.8",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "reselect": "^3.0.0",
    "stampit": "^3.1.2",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.16.1",
    "watch": "^1.0.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
